{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/episodios/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },

    ]
  }